\fancypagestyle { plain }
  {
    \pagestyle { fancy }
    \fancyhf { }
%<*xduugthesis>
    \fancyhead [ CE ] { \@@_rm_family: \zihao { 5  } \l_@@_pure_title_tl }
    \fancyhead [ CO ] { \@@_rm_family: \zihao { 5  } \leftmark            }
